pipeline{
    agent any
    
    tools{
        maven 'mymaven'
    }
    stages{
        stage('Checkout code'){
            steps{
                git 'https://github.com/vinaychikki46-hub/DevOpsCode.git'
            }
        }
        stage('Compile Code'){
            
            steps{
                sh 'mvn compile'
            }
        }
         stage('Test Code'){
           
            steps{
                catchError(buildResult: 'SUCCESS', stageResult: 'FAILURE'){
                sh 'mvn te'
                }
            }
           
        }
        
        stage('Package Code'){
           
            steps{
                sh 'mvn package'
            }
        }
    }
}
